/binary search method  to find a key element 
#include <iostream>
using namespace std;
int binarysearch(int arr[],int size,int key){
  int start=0;
  int end=size-1;
  int mid;
 
  while(start<=end){
     mid=(start+end)/2;
    if(arr[mid]==key){
      return mid;
    }
    else if(arr[mid]<key){
      start=mid+1;
    }
    else{
      end=mid-1;
    }
  }
  return -1;
}
int main(){
   int arr[8]={2,4,6,8,10,12,14,16};
   int key;
    cin>>key;
   int b=binarysearch(arr,8,key);
   cout<<b;

  return 0;
}
